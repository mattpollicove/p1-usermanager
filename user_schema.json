{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PingOne User (example)",
  "type": "object",
  "required": ["username"],
  "properties": {
    "username": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "name": {
      "type": "object",
      "properties": {
        "given": { "type": "string" },
        "family": { "type": "string" }
      }
    },
    "phoneNumbers": {
      "oneOf": [
        { "type": "string" },
        {
          "type": "array",
          "items": { "type": "string" }
        }
      ]
    },
    "population": {
      "type": "object",
      "properties": {
        "id": { "type": "string" },
        "name": { "type": "string" }
      }
    },
    "address": {
      "type": "object",
      "properties": {
        "street": { "type": "string" },
        "city": { "type": "string" }
      }
    }
  }
}
